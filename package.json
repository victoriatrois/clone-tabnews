  "version": "1.0.0",
  "description": "Developing https://tabnews.com.br for professional growth objectives.",
  "main": "index.js",
  "scripts": {
    "dev": "npm run services:up && next dev",
    "services:up": "docker compose -f infra/compose.yaml up -d",
