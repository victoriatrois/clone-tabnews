  "license": "MIT",
  "dependencies": {
    "module-alias": "^2.2.3",
    "next": "^13.1.6",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.24.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.21",
    "@types/react": "^18.2.60",
    "@types/react-dom": "^18.2.19",
    "jest": "^29.6.2",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  }
}
